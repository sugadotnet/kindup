@using APPBASE.Helpers
@using APPBASE.Svcutil
@using APPBASE.Svcbiz
@using APPBASE.Models

@if (hlpConfig.SessionInfo.getAppResId() == null) {

    Html.RenderPartial("_Userpanel_Generic");
}
@if (hlpConfig.SessionInfo.getAppResId() != null) {
    if (hlpConfig.SessionInfo.getAppRoleId() == valFLAG.FLAG_ROLE_P)
    {
        <!-- Sidebar user panel reguler -->
        Html.RenderPartial("_Userpanel_P");
    }
    else {
        <!-- Sidebar user panel reguler -->
        Html.RenderPartial("_Userpanel_Others");
    }
}
