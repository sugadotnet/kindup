@model APPBASE.Models.Transaction_indetailVM
@using APPBASE.Helpers

@using (Html.BeginForm("Edit", "Transaction_inspp", FormMethod.Post))
{

<input type="hidden" id="ID" name="ID" value="@Model.ID" />    
<div class="box box-info">
    <div class="box-header">
        <h3 class="box-title">Edit Transaksi Pembayaran</h3>
    </div><!-- /.box-header -->
    <div class="box-body">
        @{Html.RenderPartial("_Inputs");}
    </div><!-- /.box-body -->
    <div class="box-footer">
        <div class="row">
            <div class="col-xs-12">
                <button type="submit" class="btn btn-danger btn-flat">Edit Data</button>
                <a href="@Url.Action("Create", new { id = Model.STUDENT_ID, id2 = Model.YEAR_ID })" class="btn btn-flat btn-warning">Batalkan</a>
            </div>
        </div><!-- /.row -->
    </div><!-- /.box-footer -->
</div><!-- /.box -->
}
