@using APPBASE.Helpers
@using APPBASE.Svcbiz

@{
    ViewBag.Title = "Data Siswa Alumni";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section includeBOTTOM {
    @Scripts.Render("~/bundles/JSInputMask")
}

@if (ViewBag.CRUDSavedOrDelete == valFLAG.FLAG_TRUE) {
      Html.RenderPartial("_PartialConfirmSaved"); 
}

<div class="row">
    <div class="col-sm-12">
        @using (Html.BeginForm("Index_alumni", "Student", FormMethod.Post, new { id = "frmFilter" }))
        {
            Html.RenderPartial("Index_alumni_filter");
        }
    </div> <!-- /.col-sm-12 col-md-4 -->
</div> <!-- ./row -->

<div class="row">
    <div class="col-sm-12">
        @{Html.RenderPartial("Index_alumni_details_");}
    </div> <!-- /.col-sm-12 col-md-8 -->
</div> <!-- ./row -->


<!-- Modal -->
@{Html.RenderPartial("_PartialDialogLookup");}

@section scripts {

<!-- Modal -->
@{Html.RenderPartial("_Events");}

}
